{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cross-validate rotate",
            "program": "${workspaceFolder}/.build/debug/agk",
            "args": [
                "cross-validate",
                "-m",
                "rotate",
                "-d",
                "truncated-dataset-normalized.txt"
            ],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "swift"
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "preLaunchTask": "swift-build",
            "name": "Cross-validate transe",
            "program": "${workspaceFolder}/.build/debug/agk",
            "args": [
                "cross-validate",
                "-m",
                "transe",
                "-d",
                "truncated-dataset-normalized.txt"
            ],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "swift"
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "preLaunchTask": "swift-build",
            "name": "Cross-validate gcn",
            "program": "${workspaceFolder}/.build/debug/agk",
            "args": [
                "cross-validate",
                "-m",
                "gcn",
                "-d",
                "deduplicated-dataset.txt"
            ],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "swift"
            ]
            // "env": {
            //     "CUDA_VISIBLE_DEVICES": "-1"
            // }
        },
        {
            "type": "lldb",
            "request": "launch",
            "preLaunchTask": "swift-build",
            "name": "Generate gcn report",
            "program": "${workspaceFolder}/.build/debug/agk",
            "args": [
                "restructure-report",
                "raw-reports/gcn-truncated.txt",
                "reports/gcn-truncated.txt"
            ],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "swift"
            ]
        }
    ]
}
